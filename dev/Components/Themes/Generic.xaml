                                          RenderTransformOrigin=".5,.5">
                            <ContentPresenter.RenderTransform>
                                <ScaleTransform x:Name="CopyToClipboardSuccessTextScaleTransform" />
                            </ContentPresenter.RenderTransform>
                            <FontIcon FontSize="{TemplateBinding FontSize}"
                                      Glyph="" />
                        </ContentPresenter>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
